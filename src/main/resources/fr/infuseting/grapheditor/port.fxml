<?xml version="1.0" encoding="UTF-8"?>

<?import java.lang.*?>
<?import java.util.*?>
<?import javafx.scene.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>


<?import javafx.geometry.Insets?>
<HBox id="left_port" xmlns="http://javafx.com/javafx"
      xmlns:fx="http://javafx.com/fxml" maxHeight="100">
<padding>
    <Insets bottom="10.0" top="10.0" />
</padding>
<children>
    <GridPane fx:id="ColorPort">
        <columnConstraints>
            <ColumnConstraints hgrow="SOMETIMES" minWidth="10.0" prefWidth="100.0" />
            <ColumnConstraints hgrow="SOMETIMES" minWidth="10.0" prefWidth="100.0" />
        </columnConstraints>
        <rowConstraints>
            <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
        </rowConstraints>
        <children>
            <Pane fx:id="colorPane" prefHeight="200.0" prefWidth="200.0" />
            <Label fx:id="name" text="X" />
        </children>
    </GridPane>
</children>
</HBox>
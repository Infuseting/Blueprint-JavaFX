<?xml version="1.0" encoding="UTF-8"?>

<?import java.lang.*?>
<?import java.util.*?>
<?import javafx.scene.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>


<MenuBar BorderPane.alignment="CENTER" xmlns="http://javafx.com/javafx"
         xmlns:fx="http://javafx.com/fxml"
         fx:controller="fr.infuseting.grapheditor.controller.MenusController">
    <menus>
        <Menu mnemonicParsing="false" text="File">
            <items>
                <MenuItem fx:id="loadFileBtn" mnemonicParsing="false" text="Load" />
                <MenuItem fx:id="saveFileBtn" mnemonicParsing="false" text="Save" />
                <MenuItem fx:id="exportFileBtn" mnemonicParsing="false" text="Export" />
                <SeparatorMenuItem mnemonicParsing="false" />
                <MenuItem fx:id="closeBtn" mnemonicParsing="false" text="Close" />
            </items>
        </Menu>
        <Menu mnemonicParsing="false" text="Help">
            <items>
                <MenuItem fx:id="aboutBtn" mnemonicParsing="false" text="About" />
            </items>
        </Menu>
    </menus>
</MenuBar>
